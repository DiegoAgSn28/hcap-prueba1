import numpy as np
def convulucion(A,B):
    
    C=0
    for n in range(3):
        for i in range(3):
            for j in range(3):
                C+=(A[i+n][j]*B[i][j])

        print(C)


matriz=[[6,9,0,3],[8,4,9,1],[4,1,3,12],[3,2,1,100]]
filtro=[[1,0,2],[5,0,9],[6,2,1]]

A=np.array(matriz)
B=np.array(filtro)

C=np.zeros((2,2))

resultado=convulucion(A,B)
print(resultado)


